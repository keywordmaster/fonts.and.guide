{
  "plugins": ["simple-import-sort", "eslint-plugin-next-on-pages"],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:eslint-plugin-next-on-pages/recommended"
  ]
}
